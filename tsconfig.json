{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "module": "es2015",
    "alwaysStrict": false,
    "outDir": "artifacts",
    "sourceMap": true,
    "lib": ["es2017","es5", "es2015","ES2015.Iterable", "es7","es2015.collection"],
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "resolveJsonModule": true,
    "preserveConstEnums": true,
    "allowJs": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "skipLibCheck": false,
    "baseUrl": ".",
    "rootDir": "src",
    "types": [
      "react",
      "react-native"
    ],
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
